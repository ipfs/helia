{
  "name": "@helia/unixfs",
  "version": "6.0.0",
  "description": "A Helia-compatible wrapper for UnixFS",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/helia/tree/main/packages/unixfs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/helia.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/helia/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "IPFS"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./errors": {
      "types": "./dist/src/errors.d.ts",
      "import": "./dist/src/errors.js"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main"
  },
  "dependencies": {
    "@helia/interface": "^6.0.0",
    "@ipld/dag-pb": "^4.1.5",
    "@libp2p/interface": "^3.0.2",
    "@libp2p/logger": "^6.0.5",
    "@libp2p/utils": "^7.0.5",
    "@multiformats/murmur3": "^2.1.8",
    "hamt-sharding": "^3.0.6",
    "interface-blockstore": "^6.0.1",
    "ipfs-unixfs": "^12.0.0",
    "ipfs-unixfs-exporter": "^14.0.1",
    "ipfs-unixfs-importer": "^16.0.1",
    "it-all": "^3.0.9",
    "it-first": "^3.0.9",
    "it-glob": "^3.0.4",
    "it-last": "^3.0.9",
    "it-pipe": "^3.0.1",
    "it-to-buffer": "^4.0.10",
    "multiformats": "^13.4.1",
    "progress-events": "^1.0.1",
    "sparse-array": "^1.3.2",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "aegir": "^47.0.22",
    "blockstore-core": "^6.0.2",
    "delay": "^6.0.0",
    "iso-url": "^1.2.1",
    "it-drain": "^3.0.10",
    "wherearewe": "^2.0.1"
  },
  "browser": {
    "./dist/src/utils/glob-source.js": "./dist/src/utils/glob-source.browser.js",
    "fs": false,
    "path": false,
    "url": false
  },
  "sideEffects": false
}
