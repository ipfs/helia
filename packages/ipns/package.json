{
  "name": "@helia/ipns",
  "version": "8.2.0",
  "description": "An implementation of IPNS for Helia",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/helia/tree/main/packages/ipns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/helia.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/helia/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "IPFS"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./dns-resolvers": {
      "types": "./dist/src/dns-resolvers/index.d.ts",
      "import": "./dist/src/dns-resolvers/index.js"
    },
    "./routing": {
      "types": "./dist/src/routing/index.d.ts",
      "import": "./dist/src/routing/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "docs": "aegir docs",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main"
  },
  "dependencies": {
    "@helia/interface": "^5.2.1",
    "@libp2p/interface": "^2.2.1",
    "@libp2p/kad-dht": "^14.1.3",
    "@libp2p/logger": "^5.1.4",
    "@libp2p/peer-id": "^5.1.0",
    "@multiformats/dns": "^1.0.6",
    "interface-datastore": "^8.3.1",
    "ipns": "^10.0.0",
    "multiformats": "^13.3.1",
    "progress-events": "^1.0.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@libp2p/crypto": "^5.0.7",
    "@types/dns-packet": "^5.6.5",
    "aegir": "^45.1.1",
    "datastore-core": "^10.0.2",
    "it-drain": "^3.0.7",
    "sinon": "^19.0.2",
    "sinon-ts": "^2.0.0"
  },
  "browser": {
    "./dist/src/dns-resolvers/resolver.js": "./dist/src/dns-resolvers/resolver.browser.js"
  },
  "sideEffects": false
}
