    "interface-store": "^7.0.0",
    "it-drain": "^3.0.10",
    "it-filter": "^3.1.4",
    "it-foreach": "^2.1.4",
    "it-merge": "^3.0.12",
    "it-to-buffer": "^4.0.10",
    "libp2p": "^3.0.6",
    "mortice": "^3.3.1",
