    "aegir": "^47.0.22",
    "helia": "^6.0.0",
    "ipfs-unixfs-importer": "^16.0.1",
    "ipfsd-ctl": "^15.0.2",
    "ipns": "^10.1.2",
    "it-all": "^3.0.9",
    "it-drain": "^3.0.10",
    "it-last": "^3.0.9",
    "it-map": "^3.1.4",
    "it-to-buffer": "^4.0.10",
    "kubo": "^0.38.0",
    "kubo-rpc-client": "^5.4.0",
    "libp2p": "^3.0.6",
    "multiformats": "^13.4.1",
    "uint8arrays": "^5.1.0",
    "wherearewe": "^2.0.1"
  },
  "browser": {
    "./dist/src/fixtures/create-helia.js": "./dist/src/fixtures/create-helia.browser.js",
    "./dist/src/fixtures/create-kubo.js": "./dist/src/fixtures/create-kubo.browser.js",
    "./dist/src/bin.js": "./dist/src/index.js",
    "kubo": false
  },
  "sideEffects": false
}
