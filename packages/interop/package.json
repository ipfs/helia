    "@helia/ipns": "^9.0.0",
    "@helia/json": "^5.0.0",
    "@helia/mfs": "^6.0.0",
    "@helia/routers": "^4.0.0",
    "@helia/strings": "^5.0.0",
    "@helia/unixfs": "^6.0.0",
    "@ipld/car": "^5.4.2",
    "@ipld/dag-cbor": "^9.2.5",
    "@ipld/dag-pb": "^4.1.5",
    "@libp2p/crypto": "^5.1.12",
    "@libp2p/floodsub": "^11.0.6",
    "@libp2p/interface": "^3.0.2",
    "@libp2p/kad-dht": "^16.0.5",
    "@libp2p/keychain": "^6.0.5",
    "@libp2p/peer-id": "^6.0.3",
    "@libp2p/utils": "^7.0.5",
